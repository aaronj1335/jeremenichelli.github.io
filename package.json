{
  "name": "jeremenichelli.github.io",
  "title": "Jekyll site for jeremenichelli.github.io",
  "url": "http://jeremenichelli.github.io",
  "author": "Jeremias Menichelli",
  "copyright": "2016",
  "version": "2.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jeremenichelli/jeremenichelli.github.io.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "browserify": "^13.0.0",
    "cssmin": "^0.4.3",
    "eslint": "^1.10.3",
    "less": "^2.5.3",
    "less-plugin-clean-css": "^1.5.1",
    "ncp": "^2.0.0",
    "onchange": "^2.0.0",
    "postcss-cli": "^2.3.3",
    "uglify-js": "^2.6.1"
  },
  "bugs": {
    "url": "https://github.com/jeremenichelli/jeremenichelli.github.io/issues"
  },
  "homepage": "https://github.com/jeremenichelli/jeremenichelli.github.io#readme",
  "description": "Personal site and blog",
  "main": "index.html",
  "scripts": {
    "clean": "rm -rf assets",
    "prefavicon": "mkdir -p assets/favicon",
    "favicon": "ncp src/favicon/* assets/favicon/",
    "preimg": "mkdir -p assets/img",
    "img": "ncp src/img/logo-1x.png assets/img/logo-1x.png & ncp src/img/logo-2x.png assets/img/logo-2x.png",
    "lint": "eslint src/scripts/critical/main.js",
    "js": "browserify src/scripts/critical/main.js -o _includes/critical--js.html",
    "prejs": "npm run lint && mkdir -p assets/scripts",
    "watch:js": "onchange 'src/**/*.js' -- npm run js",
    "less:critical:about": "lessc --clean-css src/styles/critical--about.less _includes/critical--about.html",
    "less:critical:blog": "lessc --clean-css src/styles/critical--blog.less _includes/critical--blog.html",
    "less:critical:error": "lessc --clean-css src/styles/critical--error.less _includes/critical--error.html",
    "less:critical:home": "lessc --clean-css src/styles/critical--home.less _includes/critical--home.html",
    "less:critical:post": "lessc --clean-css src/styles/critical--post.less _includes/critical--post.html",
    "less:critical": "npm run less:critical:about & npm run less:critical:blog & npm run less:critical:error & npm run less:critical:home & npm run less:critical:post",
    "less:noncritical:about": "lessc --clean-css src/styles/noncritical--about.less assets/styles/about.css",
    "less:noncritical:blog": "lessc --clean-css src/styles/noncritical--blog.less assets/styles/blog.css",
    "less:noncritical:error": "lessc --clean-css src/styles/noncritical--error.less assets/styles/error.css",
    "less:noncritical:home": "lessc --clean-css src/styles/noncritical--home.less assets/styles/home.css",
    "less:noncritical:post": "lessc --clean-css src/styles/noncritical--post.less assets/styles/post.css",
    "less:noncritical": "npm run less:noncritical:about & npm run less:noncritical:blog & npm run less:noncritical:error & npm run less:noncritical:home & npm run less:noncritical:post",
    "preless": "mkdir -p assets/styles",
    "less": "npm run less:critical && npm run less:noncritical",
    "watch:less": "onchange 'src/**/*.less' -- npm run less",
    "build": "npm run less && npm run js"
  },
  "dependencies": {
    "fg-loadcss": "^0.2.4"
  }
}
